{
  "name": "MJM Bors",
  "version": "1.1",
  "author": "MJM",
  "description": "cookie",
  "permissions": [
    "cookies",
    "storage",
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "nativeMessaging"
